<div class="row">
    <div class="col-md-12">
          <h2>Categories</h2>
          <div class="row">
    <div class="col-md-12">
        <div class="table-responsive">
            <table class="table">
                <thead>
                    <tr>
                        <th>Category ID</th>
                        <th>Category Name</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each categories}}
                        <tr>
                            <td><a href="/items?categories={{this.categoryId}}">{{this.categoryId}}</a></td>
                            <td><a href="/items?categories={{this.categoryId}}">{{this.categoryName}}</a></td>
                        </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>

<div class="col-md-12 text-center">
    <strong>{{message}}</strong>
</div>

